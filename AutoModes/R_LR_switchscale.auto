#R_LR_switchscale.auto

execute R_L0_switch.auto

#picking up second box at left switch
bundle{
	serial{
		lift bottom
		intake in
	}
	serial{
		drive -10
		turn to 0
		drive 28
		turn to 90
		drive 15
	}
}

#dropping off second box at right scale
turn to 0
drive 3
turn to 90
drive 180
turn to 0
bundle{
	serial{
		wait 2
		lift scale
	}
	drive 100
}
intake out