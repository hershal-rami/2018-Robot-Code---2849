#L_*L_2xscale(outside).auto
#left bumper is on corner with exchange

#deliver cube to scale
bundle {
   intake deploy
   serial {
      drive 310
      turn to 90
   }
}
bundle {
   lift scale
   drive 25
}
intake out

#pick up a second cube (outside path)
bundle {
   lift bottom
   drive -5
}
turn to 180
drive 120
turn to 90
bundle {
   intake in
   drive 20
}


#deliver second cube
bundle {
   lift scale
   turn to 0
}
drive 100
intake out
