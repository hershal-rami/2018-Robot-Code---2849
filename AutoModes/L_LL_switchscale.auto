#L_LL_switchscale.auto

#depositing first cube on switch
bundle {
   intake deploy
   drive 168
}
turn to 90
bundle {
   lift switch
   drive 20
}
intake out

#picking up second cube
bundle {
   lift bottom
   drive -20
}
turn to 0
drive 30
turn to 90
bundle {
   drive 30
   intake in
}

#depositing second cube on scale
drive -30
turn to 0
drive 95
bundle {
   drive 10
   lift scale
}
intake out