#M_L*_vaultswitch.auto
#frame is 12 inches from right vault tape
#center of robot is on center line
intake deploy
#moving to vault
drive 5
turn TO -90
drive 12
turn TO 180
bundle{
   drive 5
   lift vault
}

intake out
bundle {
   serial {
      lift bottom
      intake in
   }
   serial {
      #picking up another cube
      drive -5
      turn TO 0
      drive 125
      turn to 90
      drive 3
   }
}

#delivering cube
drive -13
turn TO 0
bundle{
   drive 13
   lift switch
}
intake out
