#L_LR_switchscale.auto

#depositing first cube on switch
bundle {
   intake deploy
   drive 130
}
turn by 90
drive 25
turn to 0
bundle {
   drive 10
   lift switch
}
intake out

#picking up second cube
bundle {
   lift bottom
   drive -10
}
turn to 90
bundle {
   drive 3
   intake in
}

