#L_*R_2xscale.auto
#left bumper is on the corner wall
#deliver first cube to scale
bundle {
   intake deploy
   drive 210
}
turn to 90
drive 130
turn to 0
drive 80
bundle {
   drive 10
   lift scale
}
intake out

#pick up second cube
bundle {
   lift bottom
   drive -10
}
drive -80
bundle {
   turn to -90
   intake in
}

#deliver second cube
turn to 0
drive 80
bundle {
   lift scale
   drive 10
}
intake out
